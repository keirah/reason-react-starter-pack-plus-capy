{
  "name": "manning-test-2-bsb-starter",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently --names 'BSB,WEBPACK,OPEN' -c 'bgBlue.bold,bgGreen.bold' 'yarn start:bscript' 'yarn start:webpack' 'yarn open'",
    "start:bscript": "bsb -make-world -w",
    "start:webpack": "webpack-dev-server --hot",
    "clean": "bsb -clean-world",
    "build": "bsb -clean-world && bsb -make-world && NODE_ENV=production webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack -w",
    "open": "open http://localhost:8080"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "reason-react": ">=0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bs-platform": "^2.1.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^3.1.3",
    "sass": "^1.0.0-beta.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.7"
  }
}
